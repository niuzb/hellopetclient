Localization['NPC_504_motto']={SimpleChinese="I may be old and wise, but the future needs young people!",Chinese='',English='',}
Localization['NPC_505_name']={SimpleChinese="Aunt Shami",Chinese='',English='',}
Localization['NPC_505_motto']={SimpleChinese="Be careful, dear! I'll be listening for good news about you.",Chinese='',English='',}
