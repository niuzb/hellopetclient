Localization['T_501035_recievetext_6']={SimpleChinese="And these Ancients?",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501035_recievetext_7']={SimpleChinese="Ancient Faen are the rarest of all. Their powers are passed down for generations. To build an Oath with Ancient Faen is the stuff of legend.",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501035_recievetext_8']={SimpleChinese="I hope they'll be able to help me...",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501035_completetext_1']={SimpleChinese="You can equip Faen with gear, too! When fully equipped, you'll activate a Magic Field. Very handy.",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501035_completetext_2']={SimpleChinese="Science is amazing!",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501035_completetext_3']={SimpleChinese="Remember! Each piece of gear is essential to activating the field.",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501035_completetext_4']={SimpleChinese="Where can I find Faen equipment?",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501035_completetext_5']={SimpleChinese="I haven't told you yet.",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501035_completetext_6']={SimpleChinese="...",Chinese='',English='',Korean='',Japanese='',}
