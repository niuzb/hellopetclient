Localization['T_505025_recievetext_4']={SimpleChinese="Oh, good!",Chinese='',English='',Korean='',Japanese='',}
Localization['T_505025_completetext_1']={SimpleChinese="Did it work?",Chinese='',English='',Korean='',Japanese='',}
