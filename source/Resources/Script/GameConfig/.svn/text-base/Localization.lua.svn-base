Localization['T_501035_recievetext_1']={SimpleChinese="So, it looks like you've got Faen blood running through you after all.",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501035_recievetext_2']={SimpleChinese="I don't know much about my upbringing...",Chinese='',English='',Korean='',Japanese='',}
