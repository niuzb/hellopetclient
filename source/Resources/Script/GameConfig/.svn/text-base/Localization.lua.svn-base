Localization['T_501024_recievetext_1']={SimpleChinese="Defense is just as important as offense! Keep track of your attributes in battle!",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501024_recievetext_2']={SimpleChinese="I'll do that!",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501024_recievetext_3']={SimpleChinese="The wild animals have grown vicious of late. We need to find out why!",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501024_recievetext_4']={SimpleChinese="I'll look into it right away.",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501024_completetext_1']={SimpleChinese="Did you find out what drove the animals wild?",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501024_completetext_2']={SimpleChinese="Yes. Loud noises coming from the forest.",Chinese='',English='',Korean='',Japanese='',}
Localization['T_501024_completetext_3']={SimpleChinese="That must be it. We'll need to send some men to investigate further.",Chinese='',English='',Korean='',Japanese='',}
