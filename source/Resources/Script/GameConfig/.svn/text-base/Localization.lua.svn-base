Localization['T_505030_description']={SimpleChinese="Talk to Turanbot",Chinese='',English='',Korean='',Japanese='',}
Localization['T_505030_recievetext_1']={SimpleChinese="Solving Turanbot's troubles will be simple! But you should still go speak with him.",Chinese='',English='',Korean='',Japanese='',}
