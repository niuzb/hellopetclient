Localization['NPC_516_name']={SimpleChinese="Elena",Chinese='',English='',}
Localization['NPC_516_motto']={SimpleChinese="I believe in equality. For example, enough Honor Medals that you give me are equal to some of the treasures you give me.",Chinese='',English='',}
Localization['NPC_517_name']={SimpleChinese="Tanuki the Recruiter",Chinese='',English='',}
Localization['NPC_517_motto']={SimpleChinese="That you've made it this far proves your spirit. Going forward will prove your mettle.",Chinese='',English='',}
Localization['NPC_518_name']={SimpleChinese="Earl Kaiser",Chinese='',English='',}
