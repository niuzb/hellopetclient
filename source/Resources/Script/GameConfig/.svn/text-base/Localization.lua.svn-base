Localization['NPC_509_name']={SimpleChinese="Ivy",Chinese='',English='',}
Localization['NPC_509_motto']={SimpleChinese="The guardian angel of the Amethyst will always be by your side.",Chinese='',English='',}
Localization['NPC_510_name']={SimpleChinese="AI",Chinese='',English='',}
Localization['NPC_510_motto']={SimpleChinese="Beep boop. What are these things called friendship and adventure?",Chinese='',English='',}
Localization['NPC_511_name']={SimpleChinese="Turanbot",Chinese='',English='',}
Localization['NPC_511_motto']={SimpleChinese="An organic could never truly appreciate the beauty of Mecha.",Chinese='',English='',}
Localization['NPC_512_name']={SimpleChinese="Halle",Chinese='',English='',}
